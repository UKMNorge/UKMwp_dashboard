<div class="nop as-card-2 as-padding-space-3 as-margin-bottom-space-2 mt-0 mb-4" id="upcomingEvents">
	<div class="header-with-icon">
        <svg class="icon" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="lucide lucide-projector-icon lucide-projector"><path d="M5 7 3 5"/><path d="M9 6V3"/><path d="m13 7 2-2"/><circle cx="9" cy="13" r="3"/><path d="M11.83 12H20a2 2 0 0 1 2 2v4a2 2 0 0 1-2 2H4a2 2 0 0 1-2-2v-4a2 2 0 0 1 2-2h2.17"/><path d="M16 16h2"/></svg>
        <h4 class="title">Kurs og webinar</h4>
		{% if kurskommune.getTidligereArrangementer().getAntall() > 0 %}
		<div>
			<a class="title-link as-btn-hover-default" rel="noopener noreferrer" href="?visKurs=true">Se eldre arrangementer</a>	
		</div>
		{% endif %}
    </div>


	<p>
		{% if kurskommune.getKommendeArrangementer().getAntall() > 0 %}
			<div class="clearfix"></div>
			{% include "user/ArrangementListe.html.twig" with {arrangementer: kurskommune.getKommendeArrangementer().getAll()} %}
			<div class="clearfix"></div>
			<div class="mb-5"></div>
		{% else %}
			<p>Ingen planlagte kurs og webinar akkurat n√•.</p>
		{% endif %}
	</p>
</div>

<style>
.header-with-icon {
	display: flex;
}
.header-with-icon .icon {
	margin-right: 1em;
}
.header-with-icon .title {
	margin: auto;
	margin-left: 0;
}
</style>
