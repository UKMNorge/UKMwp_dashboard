<script>
	google.charts.setOnLoadCallback(videoconverter_status);

	function videoconverter_status() {
		var data = google.visualization.arrayToDataTable([
			['','Uregistrerte','Uregistrerte'],
			['',{{ registrerte_monstringer.uregistrert }},{{ registrerte_monstringer.registrert }}],
		]);
		
		var options = {
		  chartArea: {'width': '100%', 'height': '100%'},
		  isStacked: 'percent',
		  orientation: 'vertical',
          bar: { groupWidth: '100%' },
		  //pieHole: 0.4,
		  legend: 'none',
          colors: [ '#dc3545', '#28a745'],
		};
		
		var chart = new google.visualization.ColumnChart(document.getElementById('registrerte_graph'));
		chart.draw(data, options);
	}
</script>

