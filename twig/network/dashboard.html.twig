<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script>
	google.charts.load('current', {'packages': ['corechart']});
</script>

<div class="clearfix"></div>
<div class="header-dashboard">UKM Norge-admin</div>
<div class="wrap dashboard" style="margin-left:0px;margin-top:0px;">
	<div class="content">
		<div class="row">

			<div class="col-xs-12 col-sm-6 col-md-4 pull-right float-right">
				<div class="col-xs-12 col-sm-12 col-md-12" style="padding: 0">
					{% embed "network/support.html.twig" %}{% endembed %}
				</div>
				{% if messages is defined and messages|length > 0 %}
					<div class="col-xs-12 col-sm-12 col-md-12 pull-left float-left " style="padding: 0">
						{% embed "network/messages.html.twig" %}{% endembed %}
					</div>
				{% endif %}
				{% embed "network/servers/dashboard.html.twig" %}{% endembed %}
			</div>

			<div class="col-xs-12 col-sm-6 col-md-8 pull-left float-left">
				{% embed "network/innhold.html.twig" %}{% endembed %}
			</div>
		</div>
	</div>
</div>
